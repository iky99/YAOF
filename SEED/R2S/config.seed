CONFIG_PACKAGE_r8152-firmware=y
CONFIG_PACKAGE_arm-trusted-firmware-rk3328=y
CONFIG_PACKAGE_arm-trusted-firmware-rk3399=y
CONFIG_PACKAGE_arm-trusted-firmware-rockchip=y

### BASIC ###
CONFIG_PACKAGE_kmod-hwmon-pwmfan=y
